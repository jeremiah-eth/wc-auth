{
  "name": "wc-auth",
  "version": "0.1.1",
  "description": "The definitive WalletConnect Auth CLI for Base builders",
  "author": "Jeremiah <jeremiah-eth>",
  "license": "MIT",
  "homepage": "https://github.com/jeremiah-eth/wc-auth#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeremiah-eth/wc-auth.git"
  },
  "bugs": {
    "url": "https://github.com/jeremiah-eth/wc-auth/issues"
  },
  "keywords": [
    "walletconnect",
    "auth",
    "cli",
    "siwe",
    "ethereum",
    "base",
    "web3",
    "authentication",
    "oclif"
  ],
  "module": "index.ts",
  "type": "module",
  "bin": {
    "wc-auth": "./bin/run.ts"
  },
  "files": [
    "bin",
    "src"
  ],
  "scripts": {
    "start": "bun run index.ts",
    "test": "bun test",
    "lint": "eslint . --ext .ts"
  },
  "oclif": {
    "bin": "wc-auth",
    "dirname": "wc-auth",
    "commands": "./src/commands",
    "hooks": {
      "init": "./src/hooks/init/load-theme"
    },
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.10.1",
    "@types/qrcode-terminal": "^0.12.2",
    "@vitest/ui": "^4.0.12",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.12"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.2.0",
    "@oclif/core": "^4.8.0",
    "@oclif/plugin-help": "^6.2.36",
    "@oclif/plugin-plugins": "^5.4.54",
    "@walletconnect/core": "^2.23.0",
    "@walletconnect/universal-provider": "^2.23.0",
    "@walletconnect/utils": "^2.23.0",
    "chalk": "^5.6.2",
    "conf": "^15.0.2",
    "jose": "^6.1.2",
    "oclif": "^4.22.47",
    "ora": "^9.0.0",
    "qrcode-terminal": "^0.12.0",
    "table": "^6.9.0",
    "viem": "^2.39.3",
    "yaml": "^2.8.1",
    "zod": "^4.1.12"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
